{
    "workbench.colorTheme": "Atom One Dark",
    "editor.fontSize": 18,
    "editor.renderWhitespace": "boundary",
    "editor.insertSpaces": false,
    "editor.autoSurround": "brackets",
    "kotlin.compiler.jvm.target": "1.8",
    "workbench.preferredDarkColorTheme": "Atom One Dark",
    "workbench.preferredHighContrastColorTheme": "Atom One Dark",
    "workbench.preferredLightColorTheme": "Atom One Dark",
    "workbench.settings.useSplitJSON": true,
    "workbench.iconTheme": "material-icon-theme",
    "git.enableSmartCommit": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "editor.rulers": [
        64,
        80
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    }
}